{
  "name": "fiat-ui",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "lessc": "lessc src/styles/antd.less src/styles/antd.css -js",
    "prepare": "husky install; yarn typechain; yarn schema; yarn metadata",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "metadata": "node scripts/metadata.js",
    "start": "next start",
    "typechain": "typechain --target=ethers-v5 ./src/abis/**/*.json --out-dir ./types/typechain/",
    "schema": "yarn add apollo; bash scripts/apollo-schema.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "bash -c tsc",
      "git add"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "order/properties-alphabetical-order": null,
      "selector-max-id": 1,
      "selector-class-pattern": null,
      "selector-no-qualifying-type": null,
      "selector-max-compound-selectors": 8,
      "max-nesting-depth": 7
    }
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/providers": "^5.4.5",
    "@ethersproject/units": "^5.4.0",
    "@popperjs/core": "2.9.2",
    "@sentry/nextjs": "^6.19.6",
    "@xstate/react": "^1.6.3",
    "antd": "^4.18.2",
    "apollo": "^2.34.0",
    "axios": "^0.21.4",
    "bignumber.js": "^9.0.1",
    "bnc-onboard": "^1.34.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.24.0",
    "ethers": "^5.4.6",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "identicon.js": "^2.3.3",
    "lodash": "^4.17.21",
    "lottie-react": "^2.2.1",
    "next": "12",
    "nullthrows": "^1.1.1",
    "outy": "^0.1.2",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-blockies": "^1.4.1",
    "react-countdown": "^2.3.2",
    "react-device-detect": "1.17.0",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-popper": "^2.2.5",
    "react-use-storage": "^0.5.1",
    "react-youtube": "^7.14.0",
    "sanitize.css": "^12.0.1",
    "swr": "^1.1.0-beta.8",
    "ts-enum-util": "^4.0.2",
    "xstate": "^4.28.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.1.2",
    "@typechain/ethers-v5": "7.0.0",
    "@types/identicon.js": "^2.3.1",
    "@types/lodash": "^4.14.175",
    "@types/react": "17.0.17",
    "@types/react-blockies": "^1.4.1",
    "@types/react-select": "^4.0.17",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "^7.0.2",
    "lessc": "^1.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "sass": "^1.50.0",
    "stylelint": "^14.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typechain": "5.0.0",
    "typescript": "^4.3.5"
  }
}
